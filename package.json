{
	"name": "orange-stack-template",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "npm run build:css && npm run build:remix",
		"build:css": "cross-env NODE_ENV=production concurrently \"npm:generate:css:*\"",
		"build:remix": "remix build",
		"dev": "concurrently --names \"Remix,CSS\" \"npm:dev:remix\" \"npm:dev:css\"",
		"dev:css": "concurrently \"npm:generate:css:* -- --verbose --watch\"",
		"dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
		"format": "prettier --write .",
		"generate:css:base": "postcss \"app/styles/**/*.(scss|css)\" --base app/styles --dir app/dist/styles --ext css --map",
		"generate:css:routes": "postcss \"app/routes/**/*.(scss|css)\" --base app/routes --dir app/dist/styles/routes --ext css --map",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"setup:css": "cross-env NODE_ENV=development npm run build:css",
		"setup:prisma": "prisma generate && prisma migrate deploy && prisma db seed",
		"setup": "concurrently \"npm:setup:*\"",
		"start": "remix-serve build",
		"start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
		"test": "vitest",
		"test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
		"pretest:e2e:run": "npm run build",
		"test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
		"typecheck": "tsc -b && tsc -b cypress",
		"validate": "concurrently \"npm:test -- --run\" \"npm:lint\" \"npm:typecheck\" \"npm:test:e2e:run\""
	},
	"dependencies": {
		"@prisma/client": "^4.6.1",
		"@remix-run/node": "*",
		"@remix-run/react": "*",
		"@remix-run/serve": "*",
		"@remix-run/server-runtime": "*",
		"bcryptjs": "^2.4.3",
		"isbot": "^3.6.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tiny-invariant": "^1.3.1"
	},
	"devDependencies": {
		"@csstools/postcss-sass": "^5.0.1",
		"@csstools/sass-import-resolve": "^1.0.0",
		"@faker-js/faker": "^7.6.0",
		"@remix-run/dev": "*",
		"@remix-run/eslint-config": "*",
		"@testing-library/cypress": "^8.0.7",
		"@testing-library/dom": "^8.19.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/bcryptjs": "^2.4.2",
		"@types/eslint": "^8.4.10",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.9",
		"@vitejs/plugin-react": "^2.2.0",
		"@vitest/coverage-c8": "^0.24.5",
		"autoprefixer": "^10.4.13",
		"binode": "^1.0.5",
		"c8": "^7.12.0",
		"concurrently": "^7.6.0",
		"cookie": "^0.5.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.14",
		"cypress": "^10.11.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"happy-dom": "^6.0.4",
		"msw": "^0.47.4",
		"node-sass-alias-importer": "^2.0.0",
		"postcss": "^8.4.19",
		"postcss-cli": "^10.0.0",
		"postcss-import": "^15.0.0",
		"postcss-preset-env": "^7.8.3",
		"postcss-scss": "^4.0.5",
		"prettier": "2.7.1",
		"prisma": "^4.6.1",
		"sass": "^1.56.1",
		"start-server-and-test": "^1.14.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.8.4",
		"vite": "^3.2.4",
		"vite-tsconfig-paths": "^3.6.0",
		"vitest": "^0.24.5"
	},
	"engines": {
		"node": ">=14"
	},
	"prisma": {
		"seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
	}
}
